@Article{nonlinear_thin_beam,
	author		= {A. Pielorz},
	title		= {Nonlinear equations for a thin beam},
	journal		= {Acta Mechanica},
	number		= {1--2},
	volume		= {167},
	year		= {2004},
	language	= {english},
	annotation	= {Finite oscillations of a thin beam are analyzed, 
					taking into account large deflections and rotations with 
					small strains. It is shown that, depending on the 
					assumptions, problems for an elastic and viscoelastic beam 
					lead to solving hyperbolic or parabolic nonlinear partial 
					differential equations. As an example, a cantilever beam is 
					considered. The solutions for nonlinear vibrations are 
					obtained by means of the Galerkin method, and numerical 
					results are presented graphically for four material models.}
}

